pipeline {
  agent any
  stages {
    stage('Initialisation') {
      steps {
        echo 'Initialisation of Flask Application'
      }
    }
    stage('Build') {
      steps {
        sh 'echo '
        echo 'Print du build'
      }
    }
    stage('') {
      steps {
        echo 'Deploy'
        echo 'Deploy'
      }
    }
  }
}