pipeline {
  agent any
  stages {
    stage('Initialisation') {
      steps {
        echo 'Initialisation of Flask Application'
      }
    }
    stage('Build') {
      steps {
        sh 'echo '
        echo 'Print du build'
      }
    }
    stage('Deloy') {
      steps {
        echo 'Start deploy'
        echo 'End Deploy'
      }
    }
  }
}